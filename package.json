{
  "name": "@elemaudio/compressor",
  "version": "1.0.0",
  "type": "module",
  "description": "A textbook hard-knee compressor Elementary plugin",
  "author": "Nick Thompson <nick@elementary.audio>",
  "homepage": "https://www.elementary.audio",
  "license": "ISC",
  "main": "index.js",
  "files": [
    "index.js",
    "README.md"
  ],
  "publishConfig": {
    "registry": "https://www.elementary.audio/api/v1/registry/"
  },
  "targets": {
    "default": {
      "context": "browser"
    }
  },
  "scripts": {
    "start": "parcel index.html",
    "start-plugin": "cp manifest.json dist/elementary.config.json && parcel --no-cache --port 3000 --https --cert localhost-cert.pem --key localhost-key.pem index.html",
    "build": "parcel build --no-source-maps --no-cache index.html"
  },
  "browserslist": [
    ">0.2%",
    "last 2 versions",
    "not dead",
    "not op_mini all",
    "ie 10"
  ],
  "devDependencies": {
    "@elemaudio/plugin-renderer": "^1.0.2",
    "@parcel/config-default": "^2.5.0",
    "@parcel/transformer-svg-react": "^2.5.0",
    "@use-gesture/react": "^10.2.15",
    "parcel": "^2.6.0",
    "process": "^0.11.10",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.0.24",
    "zustand": "^4.0.0-rc.1"
  },
  "dependencies": {
    "@elemaudio/core": "^1.0.3"
  }
}
