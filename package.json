{
  "name": "compressor",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "A textbook hard-knee compressor Elementary plugin",
  "author": "Nick Thompson <nick@elementary.audio>",
  "homepage": "https://www.elementary.audio",
  "license": "ISC",
  "main": "index.js",
  "files": [
    "index.js",
    "README.md"
  ],
  "scripts": {
    "start": "parcel index.html",
    "start-plugin": "parcel --no-cache --port 3000 --https --cert localhost-cert.pem --key localhost-key.pem index.html",
