      "last 2 versions",
      "not dead",
      "not op_mini all",
      "ie 10"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@elemaudio/plugin-renderer": "^1.0.2",
    "@parcel/config-default": "^2.5.0",
    "@parcel/transformer-svg-react": "^2.5.0",
    "@use-gesture/react": "^10.2.15",
    "parcel": "^2.5.0",
    "process": "^0.11.10",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "resize-observer-polyfill": "^1.5.1",
    "tailwindcss": "^3.0.24",
    "zustand": "^4.0.0-rc.1"
  },
  "dependencies": {
    "@elemaudio/core": "^1.0.3"
